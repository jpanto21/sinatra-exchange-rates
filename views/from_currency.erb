

    <h1>Currency Converter</h2>

<body>
    <h1>Convert <%= @original_currency %></h1>
    <a href="/">Back</a>
    <ul>
        <% @symbols.each do |symbol, data| %>
            <% unless symbol == @original_currency %>
                <li><a href='/<%= @original_currency %>/<%= symbol %>'>Convert 1 <%= @original_currency %> to <%= symbol %>...</a></li>
            <% end %>
        <% end %>
    </ul>
